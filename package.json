{
  "name": "firebase-blog",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/Tosuke/firebase-blog.git",
  "author": "Tosuke <tasukeprg@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "dev": "ts-node server/server.ts",
    "build": "run-s build:clean build:next build:static",
    "build:clean": "rimraf .next public/_next/static functions/.next",
    "build:next": "next build",
    "build:static": "cpx \".next/static/**/*\" public/_next/static",
    "build:server": "cpx \".next/**/*\" functions/.next",
    "build:env": "cpx .env functions",
    "generate": "run-s build generate:file deploy:static",
    "generate:file": "ts-node tools/generate.ts",
    "deploy:static": "firebase deploy --only hosting",
    "analyze": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "firebase": "^5.8.6",
    "firebase-admin": "^7.0.0",
    "hoist-non-react-statics": "^3.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.0.3",
    "node-mocks-http": "^1.7.3",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.5",
    "preact-context": "^1.1.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-jsx": "^3.2.1",
    "universal-router": "^8.1.0"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.5",
    "@tosuke/prettier-config": "^1.0.1",
    "@types/dotenv-safe": "^5.0.3",
    "@types/express": "^4.16.1",
    "@types/hoist-non-react-statics": "^3.3.0",
    "@types/next": "^8.0.1",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-preact": "^0.1.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "bulma": "^0.7.4",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}
