{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/test", "function": "test"
      },
      {
        "source": "/images/**", "function": "serve"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
